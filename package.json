{
  "name": "playwrightmcp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "ai-review": "node scripts/ai-review.js",
    "ai-review:staged": "node scripts/ai-review.js --staged",
    "commit-check": "node scripts/analyze-commits.js validate",
    "commit-analyze": "node scripts/analyze-commits.js recent",
    "setup-hooks": "husky install",
    "precommit": "npm run ai-review:staged && npm run commit-check"
  },
  "keywords": ["playwright", "testing", "ai-review", "automation"],
  "author": "",
  "license": "ISC",
  "description": "Playwright testing project with AI-powered code and commit review",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.10",
    "husky": "^8.0.3",
    "@octokit/rest": "^20.0.2"
  },
  "optionalDependencies": {
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.13.1"
  }
}
